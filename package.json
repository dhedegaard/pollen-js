{
  "name": "pollen",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "tsc && copyfiles --up 3 node_modules/typeface-roboto/files/roboto-latin-{400,500,700}.woff2 static/fonts",
    "postbuild": "workbox generateSW workbox-config.js",
    "watch": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "start": "tsc && node ./build/index.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "apollo-server": "^2.12.0",
    "apollo-server-express": "^2.14.2",
    "axios": "^0.19.2",
    "csso": "^4.0.3",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "helmet": "^3.22.0",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0",
    "typeface-roboto": "^0.0.75",
    "typescript": "^3.8.3",
    "utility-types": "^3.10.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/csso": "^3.5.1",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.45",
    "@types/morgan": "^1.9.0",
    "@types/pug": "^2.0.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "4.1.8",
    "@types/xml2js": "^0.4.5",
    "copyfiles": "^2.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "workbox-cli": "^5.1.3"
  }
}
