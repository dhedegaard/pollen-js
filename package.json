    "react-dom": "^18.1.0",
    "typeface-roboto": "^1.1.13",
    "typescript": "^4.7.2",
    "utility-types": "^3.10.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
